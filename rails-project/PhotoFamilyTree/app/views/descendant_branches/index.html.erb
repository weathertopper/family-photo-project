<p id="notice" style="color: rgba(40,40,40,.8);"><%= notice %></p>

<h1>Descendant Branches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @descendant_branches.each do |descendant_branch| %>
      <tr>
        <td><%= link_to 'Show', descendant_branch %></td>
        <td><%= link_to 'Edit', edit_descendant_branch_path(descendant_branch) %></td>
        <td><%= link_to 'Destroy', descendant_branch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Descendant Branch', new_descendant_branch_path %>
